    "@vercel/analytics": "^1.5.0",
    "fast-xml-parser": "^5.2.5",
    "html2canvas": "^1.4.1",
    "i18next": "^23.7.11",
    "i18next-browser-languagedetector": "^7.2.0",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.295.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.0",
    "react-joyride": "^2.9.3",
