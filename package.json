{
  "name": "vendorsoluce",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "node_modules/.bin/vite",
    "build": "tsc && node_modules/.bin/vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:ci": "vitest --run --coverage --reporter=verbose",
    "preview": "node_modules/.bin/vite preview",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "security:check": "npm audit --audit-level=moderate",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "bundle:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "performance:test": "npm run build && npx lighthouse dist/index.html --output=json --output-path=./lighthouse-report.json",
    "accessibility:test": "npm run build && npx pa11y dist/index.html"
  },
  "dependencies": {
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.52.1",
    "@vercel/analytics": "^1.5.0",
    "dompurify": "^3.0.8",
    "fast-xml-parser": "^5.2.5",
    "html2canvas": "^1.4.1",
    "i18next": "^23.7.11",
    "i18next-browser-languagedetector": "^7.2.0",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.295.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.0",
    "react-joyride": "^2.9.3",
    "react-router-dom": "^6.21.0",
    "recharts": "^3.1.0",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lighthouse": "^12.8.1",
    "lint-staged": "^15.2.2",
    "pa11y": "^9.0.0",
    "postcss": "^8.4.32",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.3.6",
    "terser": "^5.28.1",
    "typescript": "^5.2.2",
    "vite": "^7.1.3",
    "vite-bundle-analyzer": "^0.7.0",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,json,css,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
