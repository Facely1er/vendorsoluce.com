{
  "name": "vendorsoluce",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "node_modules/.bin/vite",
    "build": "tsc && node_modules/.bin/vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "lint": "npx eslint . --max-warnings 50",
