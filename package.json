    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "preview": "node_modules/.bin/vite preview"
  },
  "dependencies": {
    "@sentry/react": "^10.5.0",
    "@sentry/tracing": "^7.120.4",
